{
  "name": "ember-hotspots",
  "version": "0.2.0",
  "description": "Create interactive prototypes from scratch and design mockups using little code but the full power of the Ember.js ecosystem.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:simplabs/ember-hotspots.git"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "npm-run-all --aggregate-output --continue-on-error --parallel format:*",
    "format:hbs": "ember-template-lint . --fix",
    "format:js": "eslint . --fix",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-funnel": "3.0.8",
    "broccoli-merge-trees": "4.2.0",
    "broccoli-persistent-filter": "3.1.3",
    "broccoli-plugin": "4.0.7",
    "broccoli-stew": "3.0.0",
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "6.1.1",
    "ember-fetch": "8.1.2",
    "glob": "8.0.3",
    "image-size": "1.0.2",
    "mkdirp": "1.0.4",
    "walk-sync": "3.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.9.3",
    "@embroider/test-setup": "1.8.3",
    "@glimmer/component": "1.1.2",
    "@glimmer/tracking": "1.1.2",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "2.5.0",
    "ember-cli": "4.9.2",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-get-config": "2.1.1",
    "ember-load-initializers": "2.1.2",
    "ember-page-title": "7.0.0",
    "ember-qunit": "6.0.0",
    "ember-resolver": "8.1.0",
    "ember-source": "4.9.3",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.3.0",
    "ember-test-selectors": "6.0.0",
    "ember-try": "2.0.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "11.3.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.4",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.1",
    "qunit": "2.19.3",
    "qunit-dom": "2.0.0",
    "release-it": "14.14.3",
    "release-it-lerna-changelog": "4.0.1",
    "webpack": "5.75.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
